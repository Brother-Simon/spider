{
  "env": {
    "mocha": true,
    "node": true
  },
  
  "rules": {
    "global-strict": 0,
    "quotes": 0,
    "camelcase": 0,
    "no-comma-dangle": 0,
    "no-trailing-spaces": 0,
    "strict": 0,
    "eol-last": 0,
    "space-infix-ops": 0,
    "consistent-return": 0,
    "no-underscore-dangle": 0,
    "key-spacing": 0,
    "no-extra-boolean-cast": 0
  }
}