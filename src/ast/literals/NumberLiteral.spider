use :node;

var Node = require('../Node').Node;

func NumberLiteral(text)
  extends Node {
  
  this.type = 'Literal';
  this.value = ::Number(text);
  this.raw = text;
}

NumberLiteral.prototype.codegen = () -> { 
  if !super.codegen() {
    return;
  }
  
  return this; 
};

NumberLiteral.prototype.hasCallExpression = () -> false;

exports.NumberLiteral = NumberLiteral;